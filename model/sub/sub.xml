        <!-- Every 2.5m there is 1 lanes -->

        <!-- full sub dimensions: 18 in x 13.3 in x 10 in-->
        <body name="sub" pos="0 0 1" euler="1.57 3.14 0">
            <freejoint/>
            <geom name="box" type="mesh" rgba="0.9 0.9 0.8 1" mesh="sub_mesh" size="0.4572 0.33782 0.254" mass="9" />
            <camera name="sub_track_cam" mode="track" pos="1 0 0"/>
            <body name="camera_link1" euler="0 3.14 0" pos="0.32 0.45 0">
                <geom name="camera_cylinder" type="cylinder" size="0.04 0.030" pos="0 0 0" rgba="1 1 0 1"/>
                <joint type="hinge" axis="0 1 0"/>
                <body name="camera_link2" euler="0 0 0" pos="-0.2 0 0">
                    <!-- <geom name="camera_cylinder2" type="cylinder" size="0.04 0.030" pos="0 0 0" rgba="1 0 0 1"/> -->
                    <camera name="sub_front_cam" mode="fixed" euler="0 1.57 0" pos="0 0 0" fovy="45"/>
                    </body>
                </body>
            <!-- <camera name="sub_tracking_cam" mode="fixed" pos="0 1 0"/> -->
            <!-- <light pos="-1 0 3" dir="1 0 -3" mode="trackcom"/> -->
            <!-- The mass of the weight was chosen to be slightly bigger than the total buoyancy of the balloons. -->
            <site name="imu" pos="0 0 0"/>
        </body>

